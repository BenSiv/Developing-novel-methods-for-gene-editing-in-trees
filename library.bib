@article{Chang2011,
abstract = {One-dimensional nanostructures are ideal building blocks for functional nanoscale assembly. Peptide-based nanofibers have great potential in building smart hierarchical structures due to their tunable structures at the single residue level and their ability to reconfigure themselves in response to environmental stimuli. We observed that pre-adsorbed silk-elastin-based protein polymers self-assemble into nanofibers through conformational changes on a mica substrate. Furthermore, we demonstrate that the rate of self-assembly was significantly enhanced by applying a nanomechanical stimulus using atomic force microscopy. The orientation of the newly grown nanofibers was mostly perpendicular to the scanning direction, implying that the new fiber assembly was locally activated with directional control. Our method provides a novel way to prepare nanofiber patterned substrates using a bottom-up approach.},
author = {Chang, Jonathan and Peng, Xiu Feng and Hijji, Karam and Cappello, Joseph and Ghandehari, Hamidreza and Solares, Santiago D. and Seog, Joonil},
doi = {10.1021/ja110191f},
issn = {00027863},
journal = {Journal of the American Chemical Society},
