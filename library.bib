journal = {International Journal of Molecular Sciences},
keywords = {AP2,Dwarf,Microarrays,Mutant,Poplar,RAV},
number = {3},
pages = {2744--2762},
pmid = {22489122},
title = {{Genome-wide analysis of a TaLEA-introduced transgenic populus simonii × populus nigra dwarf mutant}},
volume = {13},
