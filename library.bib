isbn = {0123456789},
issn = {15735028},
journal = {Plant Molecular Biology},
keywords = {Callus,De novo shoot organogenesis,Poplar,Regeneration,WUSCHEL-RELATED HOMEOBOX 11},
number = {4-5},
