keywords = {2017,2017 accepted,cacl 2 method,coli,competent cell,dna,e,hanahan,heat-shock transformation,july 1,may 1,molecular biology,plasmid,s method,submitted},
number = {June},
pages = {22--25},
title = {{Preparation of calcium competent Escherichia coli and heat-shock transformation}},
volume = {1},
year = {2017}
}
@article{Yancheva2006,
abstract = {An efficient and reproducible system for Agrobacterium-mediated transformation of the pear (Pyrus communis L.) cultivar Spadona was developed. Leaf explants of in vitro propagated plants were cocultivated with the disarmed Agrobacterium strain EHA105 harboring the plasmid pME504, carrying the uidA-intron and nptII genes. Under selective conditions, 5{\%} of the plantlets regenerated and were positively stained for GUS. However, most of the GUS-positive plants re-callused and subsequently died, leaving only 0.3-0.8{\%} of these plantlets to reach maturity. In order to identify transformed shoots at early stages of regeneration, we introduced the green fluorescent protein (GFP) into the pear cultivar Spadona using the plasmid PZP carrying the nuclear-targeted GFP and nptII genes. High expression levels of GFP were detected in transgenic cells as early as 7 days after transformation. GFP marked-callii and transformed plants were observed after 14 and 24 days, respectively. Fluorescence microscopy screening of transformed plant material, under the selection of kanamycin, increased the transformation frequency to 3.0-4.0{\%}. We conclude that the introduction of GFP improves the selection of transformed plants of Spadona pear. {\textcopyright}Springer-Verlag 2005.},
author = {Yancheva, S D and Shlizerman, L A and Golubowicz, S and Yabloviz, Z and Perl, A and Hanania, U and Flaishman, M A},
doi = {10.1007/s00299-005-0025-1},
isbn = {9723968339},
issn = {07217714},
journal = {Plant Cell Reports},
keywords = {Agrobacterium,GFP,GUS,Pear,Pyrus communis,Regeneration,Transformation},
number = {3},
pages = {183--189},
pmid = {16328389},
title = {{The use of green fluorescent protein (GFP) improves Agrobacterium-mediated transformation of 'Spadona' pear (Pyrus communis L.)}},
volume = {25},
year = {2006}
}
